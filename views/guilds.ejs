  </li>
</ul>
    </div>
  </div>
    <div class="container">
        <div class="row">
            <% servidor.map(dato => { %>
            <div class="col-md-3 col-sm-6 p-3">
                <div class="card">
                    <img src="https://cdn.discordapp.com/icons/<%=dato.id%>/<%=dato.icon%>.png" alt=""
                        class="card-img-top">
                    <div class="card-body">
                        <div class="card-title"><%= dato.name %></div>
                        <a href="https://dashi.glitch.me/sv/<%=dato.id%>" class="btn btn-success">Ajustar</a>
                    </div>
                </div>
            </div>
            <% }) %>
        </div>
    </div>

